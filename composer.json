{
  "name": "miaoxing/file",
  "type": "miaoxing-plugin",
  "description": "Miaoxing File Plugin",
  "keywords": [
    "miaoxing",
    "file"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/app": "^0.7.10"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\File\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\File\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "miaoxing/dev": "^9.0.0"
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "phpstan/extension-installer": true
    }
  },
  "extra": {
    "require-ci": {
      "miaoxing/dev": "miaoxing/dev as 8.x-dev",
      "miaoxing/phpstan-miaoxing": "miaoxing/phpstan-miaoxing as 2.x-dev",
      "miaoxing/plugin": "miaoxing/plugin as 0.14.x-dev",
      "wei/wei": "twinh/wei as 0.17.x-dev"
    }
  }
}
